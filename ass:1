class Solution:
    def isPrime (self, n):
        # code here
        import math
        arr=[True for i in range (n+1)]
        if n==0 or n==1:
            return 0
        arr[0]=arr[1]=False
        x=int(math.sqrt(n))
        for i in range(2,x+1):
            if arr[i]:
                for j in range(i*i,n+1,i):
                    arr[j]=False
        if arr[i]:
            return 1
        else:
            return 0
